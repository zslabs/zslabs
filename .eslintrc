{
  "extends": "eslint:recommended",
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-console": 0,
    "semi": ["error", "always"]
  },
  "globals": {
    "$": true
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
